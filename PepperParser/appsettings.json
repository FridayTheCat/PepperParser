{
  "ConnectionStrings": {
    "DefaultConnection": "Your Connection String"
  },

  "TimeCron": "*/15 * * * *", //Каждые 15 минут

  "UserName": "login", //Логин для доступа в админ-панель
  "NormalizedUserName": "LOGIN", //ЛОГИН
  "Password": "Pass", //Пароль

  "MailFrom": "YourEmail@Email.com", //Аккаунт откуда будут отправляться сообщения
  "MailPass": "YourPass", //Пароль от этого аккаунта (я использовал пароль приложений в mail.ru)
  "NameTo": "Name", //Имя которое будет отображаться
  "MailTo": "CompanyEmail@Email.com", //Почта на которую будут отправляться Email
  "Smtp": "smtp.mail.ru", //smtp сервер
  "SmtpPort": 465, //Порт от smtp сервера
  "UseSSL": true, //Использование ssl

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
